{
  "routes": [
    {
      "src": "/*",
      "dest": "/public/index.html"
    }
  ],
  "builds": [
    {
      "src": "public/*.html",
      "use": "@vercel/static"
    },
    {
      "src": "public/*.css",
      "use": "@vercel/static"
    },
    {
      "src": "public/*.js",
      "use": "@vercel/static"
    },
    {
      "src": "api/*.js",
      "use": "@vercel/node"
    }
  ]
}